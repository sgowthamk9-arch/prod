<?xml version="1.0" encoding="UTF-8"?>
<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingCriteriaRules>
        <fullName>Share_All_Engagements_with_ACX_App_Admin</fullName>
        <accessLevel>Edit</accessLevel>
        <description>This sharing rule share all the engagements with ACX App Admins with Read and Write access.</description>
        <label>Share_All_Engagements_with_ACX_App_Admin</label>
        <sharedTo>
            <group>ACX_App_Admins</group>
        </sharedTo>
        <criteriaItems>
            <field>Engagement_ID__c</field>
            <operation>notEqual</operation>
            <value></value>
        </criteriaItems>
        <includeRecordsOwnedByAll>true</includeRecordsOwnedByAll>
    </sharingCriteriaRules>
</SharingRules>
