<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Contact_VR03_ClientContactsOnlyClientAcc</fullName>
    <active>true</active>
    <description>JIRALAGMPB-785</description>
    <errorConditionFormula>AND(
    $RecordType.Name = &quot;Client Contact&quot;,
    Account.RecordType.Name &lt;&gt; &quot;Client&quot;
)</errorConditionFormula>
    <errorMessage>Contact_VR03: Client Contacts can only be added to Client Accounts.</errorMessage>
</ValidationRule>
