<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>File_Size_Display__c</fullName>
    <description>JIRA:AGMPB-774</description>
    <externalId>false</externalId>
    <formula>IF(File_Size__c &lt; 1048576, 
   TEXT(ROUND( File_Size__c / 1024, 2)) &amp; &apos; KB&apos;, 
   TEXT(ROUND(File_Size__c / 1048576, 2)) &amp; &apos; MB&apos;
)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>File Size Display</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
