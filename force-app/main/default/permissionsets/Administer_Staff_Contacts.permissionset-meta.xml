    </fieldPermissions>
    <fieldPermissions>
        <editable>true</editable>
        <field>Contact.LinkedIn_URL__c</field>
        <readable>true</readable>
    </fieldPermissions>
    <fieldPermissions>
        <editable>true</editable>
        <field>Contact.Staff_Level__c</field>
        <readable>true</readable>
